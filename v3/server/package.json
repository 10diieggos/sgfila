{
  "name": "sgfila-server",
  "version": "3.0.0",
  "description": "SGFILA v3 - Server (TypeScript)",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "node node_modules/tsx/dist/cli.mjs watch src/server.ts",
    "dev:win": "node_modules\\.bin\\tsx.cmd watch src/server.ts",
    "build": "node node_modules/typescript/bin/tsc",
    "start": "node dist/server.js",
    "lint": "node node_modules/eslint/bin/eslint.js src --ext .ts",
    "type-check": "node node_modules/typescript/bin/tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
