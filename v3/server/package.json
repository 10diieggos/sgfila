{
  "name": "sgfila-server",
  "version": "3.0.0",
  "description": "SGFILA v3 - Server (TypeScript)",
  "type": "module",
  "main": "dist/server/src/server.js",
  "scripts": {
    "dev": "node node_modules/tsx/dist/cli.mjs watch src/server.ts",
    "dev:win": "node_modules\\.bin\\tsx.cmd watch src/server.ts",
    "build": "node node_modules/typescript/bin/tsc",
    "start": "node dist/server/src/server.js",
    "export:dataset": "node dist/server/src/scripts/export-dataset-jsonl.js",
    "lint": "node node_modules/eslint/bin/eslint.js src --ext .ts",
    "type-check": "node node_modules/typescript/bin/tsc --noEmit",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  }
}
