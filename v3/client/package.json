{
  "name": "sgfila-client",
  "version": "3.0.0",
  "description": "SGFILA v3 - Client (Vue 3 + TypeScript)",
  "type": "module",
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "dev:win": "node_modules\\.bin\\vite.cmd",
    "build": "node node_modules/vue-tsc/bin/vue-tsc.js && node node_modules/vite/bin/vite.js build",
    "preview": "node node_modules/vite/bin/vite.js preview",
    "lint": "node node_modules/eslint/bin/eslint.js . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-pattern 'dist/**'",
    "type-check": "node node_modules/vue-tsc/bin/vue-tsc.js --noEmit"
  },
  "dependencies": {
    "socket.io-client": "^4.6.1",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vue-tsc": "^2.0.28"
  }
}
